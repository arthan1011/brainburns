{
  "version": "2.1.4",
  "compilerOptions": {
    "target": "es2015",
    "outDir": "app/",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": ["**/*.ts"]
}
